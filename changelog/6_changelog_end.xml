<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
            http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.4.xsd">   
                       
    <changeSet  author="RGRZEGORCZYK"  id="compile_schema" runAlways="true" failOnError="true" context='ALL'>
        <validCheckSum>any</validCheckSum>        
        <preConditions onFail="WARN" onSqlOutput="TEST" onFailMessage="------------------------- ENJOY !! ------------------------- " >
            <sqlCheck expectedResult="25" >
                select 15 
                  FROM DUAL;
            </sqlCheck>
        </preConditions>                  
        <sqlFile  dbms="oracle"
            encoding="UTF-8"
            endDelimiter="\n/"
            path="database/dba_scripts/compile.sql"
            splitStatements="false"
            stripComments="false"/>
      <rollback>
          SELECT 1 FROM DUAL;
      </rollback>  
    </changeSet>                                                                     
</databaseChangeLog>
